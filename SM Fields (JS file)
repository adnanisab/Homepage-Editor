				if($(this).text() == '* Issue, Request or Question') {
					$(this).parent('.row').parent('section').css('display','block');
				}
			});
		}
	// Phone
		if(window.location.href.indexOf("#hpe_$phone") != -1){// First load
			window.location.href = '/form2.asp?lid=SupportTickets&lid2=&level=1&pform=support_tickets&pkeyname=ticket_id&pkey=&fkeyname=&fkey=&eflag=Yes&wpage=&hpath=&smid=&u=&c=&lf=&x=EFD5B949B0C59BECF23E6823AF110A85#hpe_phone';	
