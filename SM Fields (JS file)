// Remove this JS after Staff testing
$(document).ready(function(){ // Editor Button
	if(window.location.href.indexOf("/responsibility.asp") != -1){
		$('.smicon_links1').each(function(){
			if($(this).text() == "Home") {
				var a = $(this).attr('href');
        			var b = a.substring(0, a.lastIndexOf('/'));
				$('.my-form > center').html('<a class="sm-hp" href="' + b + '/hpeditor.htm">Home Page Editor</a>');
			}
		});
	}
});
// Remove this JS after Staff testing

// BEGIN: Home Page Editor(hpeditor.htm)
function OpenBLPopup(pageUrl, window_type, fwidth, fheight){
	$.magnificPopup.open({
	items: [ {
		src: pageUrl,
		type: window_type
	}
  ],   
  gallery: { enabled: true },
  modal: true,
  iframe: {markup: '<div class="hp-x-popup">'+'<div class="mfp-iframe-scaler" ><div class="hpe-loader"></div>'+'<div class="mfp-close"></div>'+'<iframe style="visibility:hidden;opacity:0;" class="mfp-iframe hpe-iframe" frameborder="0" allowfullscreen></iframe>'+'</div></div>'}
});
}
$(document).ready(function(){
if($(document).width() > 1024){
	if(window.location.href.indexOf("?hle") != -1){
		var url= document.location.href;
		window.history.pushState({}, "", url.split("?hle")[0]);
		window.location.reload();
	}
	if(window.location.href.indexOf("?hp_editor") != -1){
		var pl; // Poll - Add Classes
		for (pl = 1; pl <= 100; pl++) {
			$('.polls1body[id="poll' + pl + '"]').addClass('hpe-poll' + pl);
		}
		$('span.copyright').addClass('hpe-copyright'); // Copyright - Add Classes
		$('div.contactbody').addClass('hpe-contactbody'); // Contact Information - Add Classes
		if($('.hitcounter').length) { // Hitcounter - Add Class
			$('.hitcounter').addClass('hpe-hitcounter')
		}
		if($('.PhotoGallery1').children('script:first-child').text().indexOf('GetPhoto') != -1 && $('.PhotoGallery1').length) { // Old PhotoGallery1 - Add Classes
			$('.PhotoGallery1').addClass('hpe-old-photog');
	  	}
		if($('.PhotoGallery2').children('script:first-child').text().indexOf('GetPhoto') != -1 && $('.PhotoGallery2').length) { // Old PhotoGallery2 - Add Classes
			$('.PhotoGallery2').addClass('hpe-old-photog');
		}
		if($('.PhotoGallery3').children('script:first-child').text().indexOf('GetPhoto') != -1 && $('.PhotoGallery3').length) { // Old PhotoGallery3 - Add Classes
			$('.PhotoGallery3').addClass('hpe-old-photog');
	  	}
		if($('.PhotoGallery4').children('script:first-child').text().indexOf('GetPhoto') != -1 && $('.PhotoGallery4').length) { // Old PhotoGallery4 - Add Classes
  			$('.PhotoGallery4').addClass('hpe-old-photog');
		}
		$('[class*="hpe-"]').addClass('hp-x');
		$('[class*="hpe-"]').prepend('<div class="hp-y"></div>');
		$('.hp-x').each(function(){
			$(this).children('.hp-y').css('width', $(this).width());
			$(this).children('.hp-y').css('height', $(this).height());
			if($(this).height() < 40) {
				$(this).children('.hp-y').addClass('hp-smallh');
			}
			else if($(this).height() > 999) {
				$(this).children('.hp-y').addClass('hp-longh');
			}
			if($(this).width() < 32) {
				$(this).children('.hp-y').addClass('hp-smallw');
			}
		});
		var e = setInterval(function () {
			if(readCookie('hpe_page2') == 'Yes') {
				eraseCookie('hpe_page2');
				$('.mfp-close').click();
				clearTimeout(e);
				window.top.location.reload();
			}
		}, 100);
		setInterval(function () { // Popup Loader
			if($('.hpe-loader').length) {
				setTimeout(function(){
					$('.hpe-loader').addClass('hpe-loader-start');
				},1000);
				$('.hpe-iframe').load(function(){
					setTimeout(function(){
						$('.hpe-loader').animate({opacity: 0}, 400, function() {
							$('.hpe-loader').css('display','none');
						});
						$('.hpe-iframe').css('visibility','visible');
						$('.hpe-iframe').animate({opacity: 1}, 400);
					},2000);
				});
			}
		}, 100);
		// LOGO
			if($('.hpe-logo').length){
				if($('.hpe-logo #logo img').length){
					$('.hpe-logo.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/responsibility.asp#hpe_$logo_img\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Logo</a></span></div>');
				}
				else {
					$('.hpe-logo.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/responsibility.asp#hpe_$logo\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Logo</a></span></div>');
				}
			}
		// Menu
			if($('.hpe-menu').length){
				$('.hpe-menu.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Menu&x=9CCDE8B9D9B5CE0591655D0678C5F07D#hpe_menu\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Menus</a></span></div>');
			}
		// AdGroups
			var ag;
			for (ag = 1; ag <= 100; ag++) {
				if($('.hpe-ag' + ag).length){
					$('[class*="hpe-ag' + ag + ' hp-x"] .hp-y').each(function() {
						$(this).append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=CustomersAdsGroups&lid2=&level=1&pkeyname=group_id&pkey=' + ag +'&wpage=1&hpath=AdGroup' + ag +'&smid=&u=&c=&lf=&x=121B0FB828CC71FE18F0B2F73185C625#hpe_ag\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update AdGroup' + ag +' </a></span></div>');
					});
				}
			}	
		// Poll
			var px;
			for (px = 1; px <= 100; px++) {
				if($('.hpe-poll' + px).length){
					$('.polls1body[class*="hpe-poll' + px + ' hp-x"] .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Polls&lid2=&level=0&pkeyname=&pkey=&sortflag=&wpage=1&hpath=&smid=&x=B9AD706487C8AD59FFD9BE932F4FE201#hpe_poll$' + px + '\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Poll</a></span></div>');
				}
			}	
		// Highlights
			var h;
			for (h = 1; h <= 20; h++) {
				if($('.hpe-hl' + h).length){
					$('[class*="hpe-hl' + h + ' hp-x"] .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Pages&x=12D151C473A9528CA4E6AD9A7A32934C#hpe_hl$'+ h +'\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Highlight' + h +' </a></span></div>');
				}
			}
		// Previews
			if($('.hpe-prev').length){
				$('.hpe-prev.hp-x').each(function(){
					var m = $(this).attr('data-menu').replace(/'/g,"@@").replace(/ /g,"_");
					var s = $(this).attr('data-submenu').replace(/'/g,"@@").replace(/ /g,"_");
					$(this).children('.hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Pages&x=12D151C473A9528CA4E6AD9A7A32934C#hpe_prev$$' + m + '&&' + s + '\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update ' + s.replace(/@@/g,"'").replace(/_/g," ") +' </a></span></div>');
				});
			}
		// Previews (Highlight Excluded)
			if($('.hpe-xprev').length){
				$('.hpe-xprev.hp-x').each(function(){
					var m = $(this).attr('data-menu').replace(/'/g,"@@").replace(/ /g,"_");
					var s = $(this).attr('data-submenu').replace(/'/g,"@@").replace(/ /g,"_");
					$(this).children('.hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Pages&x=12D151C473A9528CA4E6AD9A7A32934C#hpe_xprev$$' + m + '&&' + s + '\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update ' + s.replace(/@@/g,"'").replace(/_/g," ") +' </a></span></div>');
				});
			}
		// Events (New)
			if($('.hpe-event').length){
				$('.hpe-event.hp-x').each(function(){
					var m = $(this).attr('data-menu').replace(/'/g,"@@").replace(/ /g,"_");
					var s = $(this).attr('data-submenu').replace(/'/g,"@@").replace(/ /g,"_");
					$(this).children('.hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Events2&x=DAFB459A3E3C455421B6CB39F15AA5A2C#hpe_event$$' + m + '&&' + s + '\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update ' + s.replace(/@@/g,"'").replace(/_/g," ") +' </a></span></div>');
				});
			}
		// Videos - Gallery - YT(URL)
			if($('.hpe-video-g-yt-url').length){
				var vm = $('.hpe-video-g-yt-url.hp-x').attr('data-menu').replace(/'/g,"@@").replace(/ /g,"_");
				var vs = $('.hpe-video-g-yt-url.hp-x').attr('data-submenu').replace(/'/g,"@@").replace(/ /g,"_");
				$('.hpe-video-g-yt-url.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Galleries&x=3B610737DF017DAF061282FB14153934#hpe_video_g_yt_url$$' + vm + '&&' + vs + '\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Videos </a></span></div>');
			}
		// Videos - Gallery - YT(Code)
			if($('.hpe-video-g-yt-code').length){
				var vm = $('.hpe-video-g-yt-code.hp-x').attr('data-menu').replace(/'/g,"@@").replace(/ /g,"_");
				var vs = $('.hpe-video-g-yt-code.hp-x').attr('data-submenu').replace(/'/g,"@@").replace(/ /g,"_");
				$('.hpe-video-g-yt-code.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Galleries&x=3B610737DF017DAF061282FB14153934#hpe_video_g_yt_code$$' + vm + '&&' + vs + '\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Videos </a></span></div>');
			}
		// Videos - Gallery - YT(ID)
			if($('.hpe-video-g-yt-id').length){
				var vm = $('.hpe-video-g-yt-id.hp-x').attr('data-menu').replace(/'/g,"@@").replace(/ /g,"_");
				var vs = $('.hpe-video-g-yt-id.hp-x').attr('data-submenu').replace(/'/g,"@@").replace(/ /g,"_");
				$('.hpe-video-g-yt-id.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Galleries&x=3B610737DF017DAF061282FB14153934#hpe_video_g_yt_id$$' + vm + '&&' + vs + '\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Videos </a></span></div>');
			}
		// Photo - Gallery
			if($('.hpe-photo-g').length){
				var pm = $('.hpe-photo-g.hp-x').attr('data-menu').replace(/'/g,"@@").replace(/ /g,"_");
				var ps = $('.hpe-photo-g.hp-x').attr('data-submenu').replace(/'/g,"@@").replace(/ /g,"_");
				$('.hpe-photo-g.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Galleries&x=3B610737DF017DAF061282FB14153934#hpe_photo_g$$' + pm + '&&' + ps + '\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Photos </a></span></div>');
			}
		// Old PhotoGallery
			if($('.hpe-old-photog').length){
				$('.hpe-old-photog.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=PhotoGallery&x=99C0693B40F1BDC4E8BA4FF98961B98F#hpe_old_photogx\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update Photos </a></span></div>');
			}
		// ArticleGroup - without Highlight
			if($('.hpe-xartgroup').length){
				$('.hpe-xartgroup.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Pages&x=12D151C473A9528CA4E6AD9A7A32934C#hpe_xartgroup$\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update</a></span></div>');
			}
		// ArticleGroup
			if($('.hpe-artgroup').length){
				$('.hpe-artgroup.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?lid=Pages&x=12D151C473A9528CA4E6AD9A7A32934C#hpe_artgroup$\',\'iframe\');"><i class="fa fa-pencil" aria-hidden="true"></i> Update</a></span></div>');
			}
		// Contact Information
			if($('.hpe-contactbody').length){
				$('.hpe-contactbody.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/responsibility.asp#hpe_$contactbody\',\'iframe\');" title="Update Contact Information"><i class="fa fa-pencil" aria-hidden="true"></i> Update</a></span></div>');
			}
		// Copyright
			if($('.hpe-copyright').length){
				$('.hpe-copyright.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/responsibility.asp#hpe_$copyright\',\'iframe\');" title="Update Copyright"><i class="fa fa-pencil" aria-hidden="true"></i> Update</a></span></div>');
			}
		// Hitcounter
			if($('.hpe-hitcounter').length){
				$('.hpe-hitcounter.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/responsibility.asp#hpe_$hitcounter\',\'iframe\');" title="Update Hit Counter"><i class="fa fa-pencil" aria-hidden="true"></i> Update</a></span></div>');
			}
		// Email
			if($('.hpe-email').length){
				$('.hpe-email.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?bls=1&lid=SupportTickets&x=EFD5B949B0C59BECF23E6823AF110A85#hpe_$email\',\'iframe\');" title="Update Email"><i class="fa fa-pencil" aria-hidden="true"></i></a></span></div>');
			}
		// Social
			if($('.hpe-social').length){
				$('.hpe-social.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?bls=1&lid=SupportTickets&x=EFD5B949B0C59BECF23E6823AF110A85#hpe_$social\',\'iframe\');" title="Update Social Media Links"><i class="fa fa-pencil" aria-hidden="true"></i></a></span></div>');
			}
		// Phone
			if($('.hpe-phone').length){
				$('.hpe-phone.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?bls=1&lid=SupportTickets&x=EFD5B949B0C59BECF23E6823AF110A85#hpe_$phone\',\'iframe\');" title="Update Phone Number"><i class="fa fa-pencil" aria-hidden="true"></i></a></span></div>');
			}
		// Bottom Links
			if($('.hpe-bottom-links').length){
				$('.hpe-bottom-links.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?bls=1&lid=SupportTickets&x=EFD5B949B0C59BECF23E6823AF110A85#hpe_$bottom_links\',\'iframe\');" title="Update Links"><i class="fa fa-pencil" aria-hidden="true"></i></a></span></div>');
			}
		// Top Links
			if($('.hpe-top-links').length){
				$('.hpe-top-links.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?bls=1&lid=SupportTickets&x=EFD5B949B0C59BECF23E6823AF110A85#hpe_$top_links\',\'iframe\');" title="Update Links"><i class="fa fa-pencil" aria-hidden="true"></i></a></span></div>');
			}
		// Google Custom Search
			if($('.hpe-gsearch').length){
				$('.hpe-gsearch.hp-x .hp-y').append('<div class="hp-z"><span><a href="#hp_editor" onclick="OpenBLPopup(\'/list2.asp?bls=1&lid=SupportTickets&x=EFD5B949B0C59BECF23E6823AF110A85#hpe_$gsearch\',\'iframe\');" title="Any issues with Search?"><i class="fa fa-pencil" aria-hidden="true"></i></a></span></div>');
			}
			
		// Reload while closing popup window
			var s = setInterval(function () {
				$('.hp-x-popup .mfp-close').click(function(){
					if(readCookie('hpe_submit') == 'Yes') {
						eraseCookie('hpe_submit');
						if(readCookie('hpe_poll') == 'Yes') {
							eraseCookie('hpe_poll');
						}
						if(readCookie('hpe_page1') == 'Yes') {
							eraseCookie('hpe_page1');
						}
						var hr;
						for (hr = 1; hr <= 20; hr++) {
							if(readCookie('hpe_hlx' + hr) == 'Yes') {
								eraseCookie('hpe_hlx' + hr);
							}
						}
						eraseCookie('hpe_prevx');
						eraseCookie('hpe_xprevx');
						eraseCookie('hpe_video_g_yt_urlx');
						eraseCookie('hpe_video_g_yt_codex');
						eraseCookie('hpe_video_g_yt_idx');
						eraseCookie('hpe_photo_gx');
						eraseCookie('hpe_old_photogx');
						eraseCookie('hpe_xartgroupx');
						eraseCookie('hpe_artgroupx');
						eraseCookie('hpe_eventx');
						clearTimeout(s);
						window.top.location.reload();
					}
				});
			}, 10);
			$('[class*="hpe-"].hp-x .hp-y .hp-z span a').click(function(){
				setTimeout(function(){
					$('.hp-x-popup iframe.mfp-iframe').load(function(){
						$('.mfp-iframe-holder .mfp-close').css('visibility','visible');
						$('.mfp-iframe-holder .mfp-close').animate({'opacity': 1}, 300);
					});
				}, 300);
			});
	}
}
});
$(window).bind('load',function () {
	if($(document).width() > 1024){ // Box Width & Height
		if(window.location.href.indexOf("?hp_editor") != -1){
			$('a').each(function(){ // Disable other links on homepage
				if($(this).attr('href') != '#hp_editor') {
					$(this).attr('href','?hp_editor');
					$(this).removeAttr('target');
				}
			});
			setInterval(function(){
				$('.hp-x').each(function(){
					$(this).children('.hp-y').css('width', $(this).width());
					$(this).children('.hp-y').css('height', $(this).height());
					if($(this).height() < 40) {
						$(this).children('.hp-y').addClass('hp-smallh');
					}
					else if($(this).height() > 999) {
						$(this).children('.hp-y').addClass('hp-longh');
					}
					if($(this).width() < 32) {
						$(this).children('.hp-y').addClass('hp-smallw');
					}
				});
			},1000);
		}
	}
});
// END: Home Page Editor(hpeditor.htm)
	
// BEGIN: Home Page Editor(Site Manager)
	
	$(document).ready(function(){
	// Hide Site Manager after submitting "Settings"
		if(window.location.href.indexOf("/responsibility.asp") != -1 && window.location.href.indexOf("/responsibility.asp#hpe_$") == -1 && readCookie('hpe_page1') == 'Yes'){
			$('.main_wrapper').css('display','none');
			eraseCookie('hpe_page1');
			createCookie('hpe_page2','Yes');
		}
	// General
		if(window.location.href.indexOf("#hpe_") != -1){
			$('.my-form').addClass('hpe-my-form');
			$('.main_wrapper').addClass('hpe-main_wrapper');
			$('#form_header_links, .footer_links, #bottom_scroll, #top_scroll').css('display','none');
			$('#form_header_wrapper').css({'margin-left':'auto', 'width':'100%'});
			$('.form_header').css('float','none');
			if($('#submit1').length || $('#submit2').length) {		
				createCookie('hpe_submit','Yes');	
				$('#submit2').css('display','none');
			}
		}
	// LOGO
		if(window.location.href.indexOf("#hpe_$logo") != -1){// First load
			var lg = window.location.href.split('$').pop();
			window.location.href = '/form2.asp?pform=options_systems&pkeyname=sys_option_system_id&pkey=1&x=BF32639A21BC5E70B8E8BDA5456F5677#hpe_' + lg;	
		}
		if(window.location.href.indexOf("#hpe_logo") != -1){ // Inside "Settings" screen
			createCookie('hpe_page1','Yes');
			$('fieldset').addClass('fieldset_hpe_logo');
			$('#form_header_wrapper span').text('Logo');
			$('.label.col.col-4').each(function(){
				if($(this).text() == 'Company Logo' || $(this).text() == 'Company Logo (HTML)') {
					$(this).parent('.row').parent('section').attr('data-row', $(this).text());
				}
			});
		}
		if(window.location.href.indexOf("#hpe_logo_img") != -1){ // Inside "Settings" screen
			$('section[data-row="Company Logo (HTML)"]').css('display','none');
		}
	// Menu
		if(readCookie('hpe_menu') == 'Yes') {
			eraseCookie('hpe_menu');
			history.pushState('', '', "#hpe_menu");
		}
		if(window.location.href.indexOf("#hpe_menu") != -1 && window.location.href.indexOf("?lid=Menu&") != -1){
			$('a').each(function(){
				$(this).attr('href', $(this).attr('href') + '#hpe_menu');
				if($(this).children('.list_links').text() == 'home' || $(this).children('.list_links').text() == 'site manager') {
					$(this).css('display','none');
					$(this).prev('.smicon_bullet1').css('display','none');
				}
				if($(this).attr('href') == '/responsibility.asp#hpe_menu'){
					$(this).css('display','none');
					$(this).next('.smicon_bullet1').css('display','none');
				}
			});
			if($('#submit1').length || $('#submit2').length) {
				createCookie('hpe_menu','Yes');				
			}
		}
	
	// AdGroups
		if(readCookie('hpe_ag') == 'Yes') {
			eraseCookie('hpe_ag');
			history.pushState('', '', "#hpe_ag");
		}
		if(window.location.href.indexOf("#hpe_ag") != -1 && window.location.href.indexOf("?lid=CustomersAdsGroups&") != -1){
